<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.electroninc.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" />

        <activity
            android:name=".SetupActivity"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />

        <activity android:name=".ComputerVsHumanActivity" />

        <activity android:name=".ComputerVsComputerActivity" />

        <activity android:name=".HumanOfflineActivity" />

        <activity android:name=".HumanOnlineActivity" />

    </application>

</manifest>